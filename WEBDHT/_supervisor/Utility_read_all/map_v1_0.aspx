                            createWin(msg.d);
                        },
                        error: function (XMLHttpRequest, textStatus, errorThrown) {
                            //alert("errorThrown: " + errorThrown + " | textStatus: " + textStatus + " | XMLHttpRequest: " + XMLHttpRequest);
                        }
                    });
                }

                function createWin(msg) {
                    maskId = [];
            if (flag == true) {
                for (var i = 0; i < constMarkers.length; i++) {
                    constMarkers[i].setMap(null);
                }
                constMarkers = [];
            }
            var bounds = new google.maps.LatLngBounds();
            for (var post in msg) {
                var infoWindowsString = '<table cellpadding=\'2\' cellspacing=\'2\'>'
                    + '<tr><td>Site Id: </td><td>' + msg[post].SiteId + '</td></tr>'
                    + '<tr><td>Vị trí: </td><td>' + msg[post].Location + '</td></tr>'
                    + '<tr><td>Logger Id: </td><td>' + msg[post].LoggerId + '</td></tr>'
                    + '</table>';
                var myLatlng = new google.maps.LatLng(msg[post].Latitude, msg[post].Longitude);
                bounds.extend(myLatlng);
                callSvcGetLs(msg[post].LoggerId, myLatlng, infoWindowsString, msg[post].Nr);
                if (flag == false) {
                   map.fitBounds(bounds);
                }
            }
            if (flag == false) {
                flag = true;
                var treeView = $find("<%= trvSites.ClientID %>");
                var parent = treeView.get_selectedNode() || treeView;
                for (var i = 0; i < msg.length; i++) {
                    var node = new Telerik.Web.UI.RadTreeNode();
                    node.set_text(msg[i].SiteId + " - " + msg[i].Location);
                    node.set_value(msg[i].LoggerId);
                    node.set_expanded(true);
                    node.set_navigateUrl('javascript:openWin(' + msg[i].Nr + ');');
                    parent.get_nodes().add(node);
                }
                treeView.commitChanges();
            }
        }
                function callSvcGetLs(loggerId,myLatlng, infoWindowsString, mId) {
                    $.ajax({
                        type: "POST",
                        url: "../../MapWebService.asmx/Map_GetChannels_ByLoggerId",
                        data: "{'id':'" + loggerId + "'}",
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function (msg1) {
                            getLs(msg1.d,myLatlng, infoWindowsString, mId);
                           
                            
                        },
                        error: function (XMLHttpRequest, textStatus, errorThrown) {
                            //alert(errorThrown);
                        }
                    });
                }
                function getLs(msg1, myLatlng, infoWindowsString, mId) {
                   
                    is = '<table cellpadding=\'3\' cellspacing=\'3\'>';

                    is += "<tr>"
                            + "<td class=\"tblHeader\">"
                            + "Kênh"
                            + "</td>"
                            + "<td class=\"tblHeader\">"
                            + "Tên"
                            + "</td><td class=\"tblHeader\">"
                            + "Ngày giờ"
                            + "</td><td class=\"tblHeader\">"
                            + "Giá trị"
                            + "</td><td class=\"tblHeader\">"
                            + "Đơn vị"
                            + "</td></tr>";
                    
                    for (var post1 in msg1) {
                        

                        ////////////////////////////////////////////////////////////////////////
                        var v=msg1[post1].LastValue;
                        if (v==null||v=="undefined") {
                            v=0;
                        }
                        else {
                            v = v.toFixed(2);
                        }
                        var d = msg1[post1].LastTimestamp;
                        if (d==null||d=="undefined") {
                            d = '';
                        }
                        else {
                            d = dateFormat(new Date(parseInt(msg1[post1].LastTimestamp.substr(6))), "dd/mm/yyyy HH:MM:ss");
                        }
                            is += "<tr>"
                                        + "<td class=\"cellCenter\">"
                                        + msg1[post1].Id
                                        + "</td>"
                                        + "<td class=\"cellCenter\">"
                                        + msg1[post1].Name
                                        + "</td><td class=\"cellCenter imp\">"
                                        + d
                                        + "</td><td class=\"cellRight imp\">"
                                        + v
                                        + "</td><td class=\"cellLeft\">"
                                        + msg1[post1].Unit
                                        + "</td></tr>";
                        
                    }
                    is += '</table>';
                    infoWindowsString += is;
                    var constMarker = new google.maps.Marker({
                        position: myLatlng,
                        map: map,
                        icon: myIcon,
                        html: infoWindowsString
                    });

                    google.maps.event.addListener(constMarker, 'click', function () {
                        infowindow.setContent(this.html);
                        infowindow.open(map, this);
                    });

                    constMarkers.push(constMarker);
                    maskId.push(mId);
                }
                function openWin(j) {
                    var i = maskId.indexOf(j);
            map.panTo(constMarkers[i].getPosition());
            google.maps.event.trigger(constMarkers[i], 'click');
        }
        window.onload = init;
        setInterval(callSvcs, 300000);


    </script>
        </telerik:RadCodeBlock>
